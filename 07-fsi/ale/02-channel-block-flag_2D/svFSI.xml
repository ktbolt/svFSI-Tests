<?xml version="1.0" encoding="UTF-8" ?>
<svFSIFile version="0.1">

<GeneralSimulationParameters>

  <Continue_previous_simulation> 0 </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 2 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 10000 </Number_of_time_steps> 
  <Time_step_size> 1e-3 </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.50 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> true </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 10 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 

  <Increment_in_saving_restart_files> 10 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 
</GeneralSimulationParameters>

<Add_mesh name="lumen" > 

  <Mesh_file_path> mesh/channel_block/Q2_h0.02/mesh-complete.mesh.vtu  </Mesh_file_path>

  <Add_face name="inlet">
      <Face_file_path> mesh/channel_block/Q2_h0.02/mesh-surfaces/inlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="outlet">
      <Face_file_path> mesh/channel_block/Q2_h0.02/mesh-surfaces/outlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="top_wall">
      <Face_file_path> mesh/channel_block/Q2_h0.02/mesh-surfaces/top_wall.vtp </Face_file_path>
  </Add_face>

  <Add_face name="bottom_wall">
      <Face_file_path> mesh/channel_block/Q2_h0.02/mesh-surfaces/bottom_wall.vtp </Face_file_path>
  </Add_face>

  <Add_face name="sqr_wall">
      <Face_file_path> mesh/channel_block/Q2_h0.02/mesh-surfaces/sqr_wall.vtp </Face_file_path>
  </Add_face>

  <Add_face name="lumen_flag_int">
      <Face_file_path> mesh/channel_block/Q2_h0.02/mesh-surfaces/lumen_flag_int.vtp </Face_file_path>
  </Add_face>

  <Domain> 0 </Domain>

</Add_mesh>


<Add_mesh name="flag" >

  <Mesh_file_path> mesh/flag/Q2_h0.03/mesh-complete.mesh.vtu  </Mesh_file_path>

  <Add_face name="flag_fixed">
      <Face_file_path> mesh/flag/Q2_h0.03/mesh-surfaces/flag_fixed.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_flag_int">
      <Face_file_path> mesh/flag/Q2_h0.03/mesh-surfaces/wall_flag_int.vtp </Face_file_path>
  </Add_face>

  <Domain> 1 </Domain>

</Add_mesh>

<!--==================================-->
<!--           Projections            -->
<!--==================================-->

<Add_projection name="wall_flag_int" >
   <Project_from_face> lumen_flag_int </Project_from_face>
</Add_projection> 

<!--==================================-->
<!--          FSI Equatioin           -->
<!--==================================-->

<Add_equation type="FSI" > 
   <Coupled> true </Coupled>
   <Min_iterations> 3 </Min_iterations>  
   <Max_iterations> 10 </Max_iterations> 
   <Tolerance> 1e-6 </Tolerance> 

   <Domain id="0" >
      <Equation> fluid </Equation> 
      <Density> 1.18e-3 </Density> 
      <Viscosity model="Constant"> <Value> 1.82e-4  </Value> </Viscosity>
      <Backflow_stabilization_coefficient> 0.2 </Backflow_stabilization_coefficient> 
   </Domain>
   
   <Domain id="1" >
      <Equation> struct </Equation> 
      <Constitutive_model type="nHK"> </Constitutive_model> 
      <Dilational_penalty_model> M94 </Dilational_penalty_model> 
      <Density> 0.1 </Density> 
      <Elasticity_modulus> 2.5e6 </Elasticity_modulus> 
      <Poisson_ratio> 0.35 </Poisson_ratio> 
   </Domain>

   <LS type="GMRES" >
      <!--Preconditioner> Trilinos-ILUT </Preconditioner--> 
      <Tolerance> 1e-6 </Tolerance>
      <Max_iterations> 100 </Max_iterations> 
      <Krylov_space_dimension> 50 </Krylov_space_dimension>
   </LS>

   <Output type="Spatial" >
     <Displacement> true </Displacement>
     <Velocity> true </Velocity>
     <Pressure> true </Pressure>
     <Vorticity> true </Vorticity>
   </Output>

   <Output type="Alias" >
       <Displacement> FS_Displacement </Displacement>
   </Output>


   <Add_BC name="inlet" > 
      <Type> Dir </Type> 
      <Value> -51.3 </Value>
      <Zero_out_perimeter> false </Zero_out_perimeter> 
   </Add_BC> 

   <Add_BC name="top_wall" >
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Zero_out_perimeter> false </Zero_out_perimeter> 
      <Effective_direction> (0, 1, 0) </Effective_direction> 
   </Add_BC> 

   <Add_BC name="bottom_wall" >
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Zero_out_perimeter> false </Zero_out_perimeter> 
      <Effective_direction> (0, 1, 0) </Effective_direction> 
   </Add_BC> 

   <Add_BC name="sqr_wall" >
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="flag_fixed" >
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 

</Add_equation>

<!--==================================-->
<!--          mesh Equatioin          -->
<!--==================================-->

<Add_equation type="mesh" >
   <Coupled> true </Coupled>
   <Min_iterations> 1 </Min_iterations>
   <Max_iterations> 5 </Max_iterations>
   <Tolerance> 1e-4 </Tolerance>
   <Poisson_ratio> 0.3 </Poisson_ratio> 

   <LS type="CG" >
      <Tolerance> 1e-4 </Tolerance>
   </LS>

   <Output type="Spatial" >
     <Displacement> true </Displacement>
   </Output>

   <Add_BC name="inlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="outlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="top_wall" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Effective_direction> (0, 1, 0) </Effective_direction> 
   </Add_BC> 

   <Add_BC name="bottom_wall" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Effective_direction> (0, 1, 0) </Effective_direction> 
   </Add_BC> 

   <Add_BC name="sqr_wall" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

</Add_equation>

</svFSIFile>


