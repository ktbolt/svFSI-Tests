<?xml version="1.0" encoding="UTF-8" ?>
<svFSIFile version="0.1">

<GeneralSimulationParameters>
  <Continue_previous_simulation> 0 </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 3 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 100 </Number_of_time_steps> 
  <!--Number_of_time_steps> 200 </Number_of_time_steps--> 
  <Time_step_size> 0.001 </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.50 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> true </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 10 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 

  <Increment_in_saving_restart_files> 50 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 
</GeneralSimulationParameters>

<Add_mesh name="wall" > 
  <Mesh_file_path> ../../mesh/wall_mesh-complete/mesh-complete.mesh.vtu </Mesh_file_path>

  <Add_face name="wall_inlet">
      <Face_file_path> ../../mesh/wall_mesh-complete/mesh-surfaces/wall_inlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_outlet">
      <Face_file_path> ../../mesh/wall_mesh-complete/mesh-surfaces/wall_outlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_inner">
      <Face_file_path> ../../mesh/wall_mesh-complete/mesh-surfaces/wall_inner.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_outer">
      <Face_file_path> ../../mesh/wall_mesh-complete/mesh-surfaces/wall_outer.vtp </Face_file_path>
  </Add_face>

</Add_mesh>

<Add_equation type="struct" > 
   <Coupled> true </Coupled>
   <Min_iterations> 3 </Min_iterations>  
   <Max_iterations> 10 </Max_iterations> 
   <Tolerance> 1e-6 </Tolerance> 

   <Prestress> true </Prestress> 

   <Density> 1.0 </Density> 

   <Poisson_ratio> 0.49 </Poisson_ratio> 
   <Elasticity_modulus> 4000000.0 </Elasticity_modulus> 
   <Constitutive_model type="nHK"> </Constitutive_model> 
   <Dilational_penalty_model> Quad </Dilational_penalty_model> 

   <Output type="Spatial" >
     <Stress> true </Stress>
     <Displacement> true </Displacement>
   </Output>

   <LS type="GMRES" >
      <Preconditioner> FSILS  </Preconditioner>
      <Max_iterations> 50 </Max_iterations> 
      <Tolerance> 1e-4 </Tolerance>
      <Krylov_space_dimension> 50 </Krylov_space_dimension>
   </LS>

   <Add_BC name="wall_inlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="wall_outlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="wall_inner" >
      <Type> Neu </Type>
      <Time_dependence> Spatial </Time_dependence> 
      <Spatial_values_file_path> rigid_wall_mean_pressure.vtp  </Spatial_values_file_path> 
      <Follower_pressure_load> false </Follower_pressure_load> 
   </Add_BC>

</Add_equation>

</svFSIFile>


