<?xml version="1.0" encoding="UTF-8" ?>
<svFSIFile version="0.1">

<GeneralSimulationParameters>
  <Continue_previous_simulation> 1 </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 3 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 3000 </Number_of_time_steps> 
  <Time_step_size> 0.001 </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.50 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> true </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 10 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 

  <Increment_in_saving_restart_files> 10 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 
</GeneralSimulationParameters>

<!-- 
====================  L u m e n   M e s h  ==================== 
-->

<Add_mesh name="lumen" > 
  <Mesh_file_path> ../../mesh/lumen_mesh-complete/mesh-complete.mesh.vtu   </Mesh_file_path>

  <Add_face name="lumen_inlet">
      <Face_file_path> ../../mesh/lumen_mesh-complete/mesh-surfaces/lumen_inlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="lumen_outlet">
      <Face_file_path> ../../mesh/lumen_mesh-complete/mesh-surfaces/lumen_outlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="lumen_wall">
      <Face_file_path> ../../mesh/lumen_mesh-complete/mesh-surfaces/lumen_wall.vtp </Face_file_path>
  </Add_face>

  <Initial_velocities_file_path> init/rigid_wall_flow.vtu </Initial_velocities_file_path> 
  <Initial_pressures_file_path>  init/rigid_wall_flow.vtu </Initial_pressures_file_path>  

  <Domain> 1 </Domain>

</Add_mesh>

<!-- 
====================  W a l l    M e s h  ==================== 
-->

<Add_mesh name="wall" >

  <Mesh_file_path> ../../mesh/wall_mesh-complete/mesh-complete.mesh.vtu </Mesh_file_path>

  <Add_face name="wall_inlet">
      <Face_file_path> ../../mesh/wall_mesh-complete/mesh-surfaces/wall_inlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_outlet">
      <Face_file_path> ../../mesh/wall_mesh-complete/mesh-surfaces/wall_outlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_inner">
      <Face_file_path> ../../mesh/wall_mesh-complete/mesh-surfaces/wall_inner.vtp  </Face_file_path>
  </Add_face>

  <Add_face name="wall_outer">
      <Face_file_path> ../../mesh/wall_mesh-complete/mesh-surfaces/wall_outer.vtp </Face_file_path>
  </Add_face>

  <Prestress_file_path> init/wall_prestress.vtu </Prestress_file_path> 

  <Initial_displacements_file_path> init/wall_prestress.vtu </Initial_displacements_file_path> 

  <Domain> 2 </Domain>

</Add_mesh>

<Add_projection name="wall_inner" >
   <Project_from_face> lumen_wall </Project_from_face>
   <Projection_tolerance> 1e-6 </Projection_tolerance> 
</Add_projection> 

<Add_equation type="FSI" > 
   <Coupled> true </Coupled>
   <Min_iterations> 3 </Min_iterations>  
   <Max_iterations> 10 </Max_iterations> 
   <Tolerance> 1e-4 </Tolerance> 

   <Domain id="1" >
       <Equation> fluid </Equation> 
       <Density> 1.0e6 </Density> 
       <Viscosity model="Constant" >
           <Value> 0.04 </Value>
       </Viscosity>
       <Backflow_stabilization_coefficient> 0.2 </Backflow_stabilization_coefficient> 
   </Domain>
   
   <Domain id="2" >
       <Equation> struct </Equation> 
       <Density> 1.0 </Density> 
       <Poisson_ratio> 0.49 </Poisson_ratio> 
       <Elasticity_modulus> 4000000.0 </Elasticity_modulus> 
       <Constitutive_model type="nHK"> </Constitutive_model> 
       <Dilational_penalty_model> Quad </Dilational_penalty_model> 
   </Domain>

   <Output type="Spatial" >
     <Velocity> true </Velocity>
     <Pressure> true </Pressure>
     <Displacement> true </Displacement>
     <WSS> true </WSS>
   </Output>

   <LS type="GMRES" >
      <Preconditioner> FSILS </Preconditioner>
      <Max_iterations> 100 </Max_iterations> 
      <Tolerance> 1e-4 </Tolerance>
      <Krylov_space_dimension> 50 </Krylov_space_dimension>
   </LS>

   <Initialize_RCR_from_flow> true </Initialize_RCR_from_flow>

   <Add_BC name="lumen_inlet" > 
      <Type> Dir </Type> 
      <Time_dependence> Unsteady </Time_dependence> 
      <Temporal_values_file_path> lumen_inlet.flow </Temporal_values_file_path> 
      <Profile> Parabolic </Profile> 
      <Impose_flux> true </Impose_flux> 
   </Add_BC> 

   <Add_BC name="lumen_outlet" > 
      <Type> Neu </Type> 
      <Time_dependence> RCR </Time_dependence> 
      <RCR_values> 
        <Proximal_resistance> 121.0 </Proximal_resistance>
        <Capacitance> 1.5e-5 </Capacitance>
        <Distal_resistance> 1212.0 </Distal_resistance>
      </RCR_values> 
      <Distal_pressure> 0.0 </Distal_pressure> 
   </Add_BC> 

   <Add_BC name="wall_inlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value>
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 

   <Add_BC name="wall_outlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value>
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 

</Add_equation>


<Add_equation type="mesh" >
   <Coupled> true </Coupled>
   <Min_iterations> 2 </Min_iterations>
   <Max_iterations> 5 </Max_iterations>
   <Tolerance> 1e-4 </Tolerance>
   <Poisson_ratio> 0.3 </Poisson_ratio> 

   <LS type="CG" >
      <Preconditioner> FSILS </Preconditioner>
      <Tolerance> 1e-4 </Tolerance>
   </LS>

   <Output type="Spatial" >
     <Displacement> true </Displacement>
   </Output>

   <Add_BC name="lumen_inlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="lumen_outlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

</Add_equation>

</svFSIFile>


