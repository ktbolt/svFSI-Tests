<?xml version="1.0" encoding="UTF-8" ?>
<svFSIFile version="0.1">

<GeneralSimulationParameters>

  <Continue_previous_simulation> 0 </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 2 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 10 </Number_of_time_steps> 
  <!--Number_of_time_steps> 300 </Number_of_time_steps--> 
  <Time_step_size> 1e-2 </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.50 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> true </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 1 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 

  <Increment_in_saving_restart_files> 10 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 
</GeneralSimulationParameters>

<Add_mesh name="lumen" > 

  <Mesh_file_path> mesh/channel/mesh-complete.mesh.vtu </Mesh_file_path>

  <Add_face name="inlet">
      <Face_file_path> mesh/channel/mesh-surfaces/inlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="outlet">
      <Face_file_path> mesh/channel/mesh-surfaces/outlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="top_wall">
      <Face_file_path> mesh/channel/mesh-surfaces/top_wall.vtp </Face_file_path>
  </Add_face>

  <Add_face name="bottom_wall">
      <Face_file_path> mesh/channel/mesh-surfaces/bottom_wall.vtp </Face_file_path>
  </Add_face>

  <Add_face name="top_leaflet">
      <Face_file_path> mesh/channel/mesh-surfaces/top_leaflet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="bottom_leaflet">
      <Face_file_path> mesh/channel/mesh-surfaces/bottom_leaflet.vtp </Face_file_path>
  </Add_face>

  <Domain> 0 </Domain>

</Add_mesh>


<Add_mesh name="leaflets" >

  <Mesh_file_path> mesh/leaflets/mesh-complete.mesh.vtu  </Mesh_file_path>

  <Add_face name="bottom_leaflet_wall">
      <Face_file_path> mesh/leaflets/mesh-surfaces/bottom_leaflet_wall.vtp </Face_file_path>
  </Add_face>

  <Add_face name="top_leaflet_wall">
      <Face_file_path> mesh/leaflets/mesh-surfaces/top_leaflet_wall.vtp </Face_file_path>
  </Add_face>

  <Add_face name="top_leaflet_int">
      <Face_file_path> mesh/leaflets/mesh-surfaces/top_leaflet_int.vtp </Face_file_path>
  </Add_face>

  <Add_face name="bottom_leaflet_int">
      <Face_file_path> mesh/leaflets/mesh-surfaces/bottom_leaflet_int.vtp </Face_file_path>
  </Add_face>

  <Domain> 1 </Domain>

</Add_mesh>

<!--==================================-->
<!--           Projections            -->
<!--==================================-->

<Add_projection name="top_leaflet_int" >
   <Project_from_face> top_leaflet </Project_from_face>
</Add_projection> 

<Add_projection name="bottom_leaflet_int" >
   <Project_from_face> bottom_leaflet </Project_from_face>
</Add_projection> 

<!--==================================-->
<!--          FSI Equatioin           -->
<!--==================================-->

<Add_equation type="FSI" > 
   <Coupled> true </Coupled>
   <Min_iterations> 3 </Min_iterations>  
   <Max_iterations> 10 </Max_iterations> 
   <Tolerance> 1e-6 </Tolerance> 

   <Domain id="0" >
      <Equation> fluid </Equation> 
      <Density> 100.0 </Density> 
      <Viscosity model="Constant"> <Value> 10.0 </Value> </Viscosity>
      <Backflow_stabilization_coefficient> 0.3 </Backflow_stabilization_coefficient> 
   </Domain>
   
   <Domain id="1" >
      <Equation> struct </Equation> 
      <Constitutive_model type="stVK"> </Constitutive_model> 
      <Density> 100.0 </Density> 
      <Elasticity_modulus> 5.6e7 </Elasticity_modulus> 
      <Poisson_ratio> 0.4 </Poisson_ratio> 
   </Domain>

   <LS type="GMRES" >
      <!--Preconditioner> Trilinos-ILUT </Preconditioner--> 
      <Tolerance> 1e-6 </Tolerance>
      <Max_iterations> 100 </Max_iterations> 
      <Krylov_space_dimension> 50 </Krylov_space_dimension>
   </LS>

   <Output type="Spatial" >
     <Displacement> true </Displacement>
     <Velocity> true </Velocity>
     <Pressure> true </Pressure>
     <Vorticity> true </Vorticity>
   </Output>

   <Output type="Alias" >
       <Displacement> FS_Displacement </Displacement>
   </Output>

   <Add_BC name="inlet" > 
      <Type> Dir </Type> 
      <Time_dependence> Unsteady </Time_dependence> 
      <Temporal_values_file_path> inflow.dat </Temporal_values_file_path>
      <Profile> Parabolic </Profile> 
   </Add_BC> 

   <Add_BC name="top_wall" >
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="bottom_wall" >
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="top_leaflet_wall" >
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 

   <Add_BC name="bottom_leaflet_wall" >
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 

</Add_equation>

<!--==================================-->
<!--          mesh Equatioin          -->
<!--==================================-->

<Add_equation type="mesh" >
   <Coupled> true </Coupled>
   <Min_iterations> 1 </Min_iterations>
   <Max_iterations> 5 </Max_iterations>
   <Tolerance> 1e-4 </Tolerance>
   <Poisson_ratio> 0.3 </Poisson_ratio> 

   <LS type="CG" >
      <Tolerance> 1e-4 </Tolerance>
   </LS>

   <Output type="Spatial" >
     <Displacement> true </Displacement>
   </Output>

   <Add_BC name="inlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="outlet" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="top_wall" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="bottom_wall" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="top_leaflet_wall" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="bottom_leaflet_wall" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

</Add_equation>

</svFSIFile>


