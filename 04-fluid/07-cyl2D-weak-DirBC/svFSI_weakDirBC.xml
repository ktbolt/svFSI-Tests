<?xml version="1.0" encoding="UTF-8" ?>
<svFSIFile version="0.1">

<GeneralSimulationParameters>
  <Continue_previous_simulation> 0 </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 2 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 10000 </Number_of_time_steps> 
  <Time_step_size> 1e-2  </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.50 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> 1 </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 100 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 

  <Save_results_in_folder> results_test_weak </Save_results_in_folder> 

  <Increment_in_saving_restart_files> 1000 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 
</GeneralSimulationParameters>

<Add_mesh name="msh" > 

  <Mesh_file_path> mesh/mesh-complete.mesh.vtu </Mesh_file_path>

  <Add_face name="inlet">
      <Face_file_path> mesh/mesh-surfaces/inlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="outlet">
      <Face_file_path> mesh/mesh-surfaces/outlet.vtp </Face_file_path>
  </Add_face>

  <Add_face name="top">
      <Face_file_path> mesh/mesh-surfaces/top.vtp </Face_file_path>
  </Add_face>

  <Add_face name="bottom">
      <Face_file_path> mesh/mesh-surfaces/bottom.vtp </Face_file_path>
  </Add_face>

  <Add_face name="cyl_wall">
      <Face_file_path> mesh/mesh-surfaces/wall.vtp </Face_file_path>
  </Add_face>

</Add_mesh>


<Add_equation type="fluid" > 

   <Coupled> true </Coupled>
   <Min_iterations> 3 </Min_iterations>  
   <Max_iterations> 10 </Max_iterations> 
   <Tolerance> 1e-3 </Tolerance> 

   <Density> 1.06 </Density> 
   <Viscosity model="Constant" > <Value> 0.04 </Value> </Viscosity>

   <Output type="Spatial" >
     <Pressure> true </Pressure>
     <Velocity> true </Velocity>
     <Vorticity> true </Vorticity>
   </Output>

   <Output type="Boundary_integral" >
     <Pressure> true </Pressure>
     <WSS> true </WSS>
   </Output>

   <LS type="BIPN" >
      <Max_iterations> 10 </Max_iterations> 

      <NS_GM_max_iterations> 3 </NS_GM_max_iterations> 
      <NS_CG_max_iterations> 500 </NS_CG_max_iterations> 

      <Tolerance> 1e-3 </Tolerance>

      <NS_GM_tolerance> 1e-3 </NS_GM_tolerance>
      <NS_CG_tolerance> 1e-3 </NS_CG_tolerance> 

      <Krylov_space_dimension> 50 </Krylov_space_dimension>
   </LS>

   <Add_BC name="inlet" > 
      <Type> Dir </Type> 
      <Profile> Flat </Profile> 
      <Value> -1.0 </Value> 
      <Zero_out_perimeter> false </Zero_out_perimeter> 
   </Add_BC> 

   <Add_BC name="top" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Effective_direction> (0, 1, 0) </Effective_direction> 
      <Zero_out_perimeter> false </Zero_out_perimeter> 
   </Add_BC> 

   <Add_BC name="bottom" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
      <Effective_direction> (0, 1, 0) </Effective_direction> 
      <Zero_out_perimeter> false </Zero_out_perimeter> 
   </Add_BC> 

   <Add_BC name="cyl_wall" > 
      <Type> Dir </Type> 
      <Time_dependence> Steady </Time_dependence> 
      <Value> 0.0 </Value> 
      <Weakly_applied> true </Weakly_applied> 
      <Penalty_parameter> 1000.0 </Penalty_parameter> 
   </Add_BC> 

</Add_equation>

</svFSIFile>


